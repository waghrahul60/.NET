<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="103*"/>
            <ColumnDefinition Width="293*"/>
        </Grid.ColumnDefinitions>
        <WebBrowser x:Name="demo" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="586" Source="https://freestylecoder.com" Grid.Column="1"/>
        <Label x:Name="lblRegister" Content="Register Here!!!" HorizontalAlignment="Left" Margin="60,35,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnShowMessage" Content="Button" HorizontalAlignment="Left" Margin="60,80,0,0" VerticalAlignment="Top" Width="82" Height="28" Click="btnShowMessage_Click"/>

    </Grid>
</Window>
